{
  "manifest_version": 3,
  "name": "TodoList Extension",
  "version": "1.0.0",
  "description": "A Todo List extension",
  "oauth2": {
    "client_id": "455449749063-u8n0q9hjkjf2fiv3fi4e39flitdkgns3.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
